{
  "name": "hp-shared",
  "version": "1.2.0",
  "scripts": {
    "refresh": "pnpm install",
    "refresh:build": "pnpm install && pnpm run build",
    "dev": "pnpm run build --watch",
    "build": "rollup --config --bundleConfigAsCjs"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "chalk": "^5.3.0",
    "idb-keyval": "^6.2.1",
    "js-cookie": "^3.0.5",
    "node-clipboardy": "^1.0.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "fast-glob": "^3.3.2",
    "npm-run-all": "^4.1.5",
    "rollup": "^4.9.5"
  },
  "exports": {
    ".": {
      "browser": "./src/index-browser.js",
      "node": "./dist/node/index.js"
    },
    "./base": {
      "browser": "./src/base/index.js",
      "node": "./dist/node/base.js"
    },
    "./dev": {
      "browser": "./src/dev/browser/index.js",
      "node": "./dist/node/dev.js"
    },
    "./storage": {
      "browser": "./src/storage/browser/index.js",
      "node": "./dist/node/storage.js"
    },
    "./*": "./*"
  },
  "unpkg": "./dist/browser/index.umd.js",
