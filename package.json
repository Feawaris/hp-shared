{
  "name": "hp-shared",
  "version": "1.5.3",
  "license": "MIT",
  "bin": "./bin/index.cjs",
  "scripts": {
    "refresh": "pnpm install && husky && pnpm run build",
    "update": "pnpx npm-check-updates --format group --upgrade && (cd docs && npm-check-updates --target newest --format group --upgrade) && pnpm run refresh",
    "clean:ignoredConfigFiles": "pnpx rimraf .markdownlint.json .markdownlint-cli2.cjs .markdownlint-cli2.cjs .markdownlintignore stylelint.config.cjs .stylelintrc.json .stylelintignore eslint.config.cjs .eslintrc.cjs .eslintignore prettier.config.cjs .prettierignore commitlint.config.cjs",
    "lint": "pnpm run lint:ignore ; pnpm run lint:md ; pnpm run lint:css ; pnpm run lint:js ; pnpm run lint:prettier ; pnpm run lint:git",
    "lint:fix": "pnpm run lint:ignore ; pnpm run lint:md:fix ; pnpm run lint:css:fix ; pnpm run lint:js:fix ; pnpm run lint:prettier:fix ; pnpm run lint:git",
    "lint:ignore": "node config/lint-ignore.cjs",
    "lint:md": "node config/lint-md.cjs && markdownlint-cli2 '**/*.{md,markdown}'",
    "lint:md:fix": "node config/lint-md.cjs && markdownlint-cli2 '**/*.{md,markdown}' --fix",
    "lint:css": "node config/lint-css.cjs && stylelint '**/*.{css,vue}'",
    "lint:css:fix": "node config/lint-css.cjs && stylelint '**/*.{css,vue}' --fix",
    "lint:js": "node config/lint-js.cjs && eslint",
    "lint:js:fix": "node config/lint-js.cjs && eslint --fix",
    "lint:prettier": "node config/lint-prettier.cjs && prettier --check .",
    "lint:prettier:fix": "node config/lint-prettier.cjs && prettier --check . --write",
    "lint:git": "node config/lint-git.cjs && echo 'test: commitlint' | commitlint",
    "build": "rollup --config --configPlugin @rollup/plugin-typescript --bundleConfigAsCjs",
    "build:watch": "pnpm run build --watch",
    "docs:dev": "pnpm -F docs run dev",
    "docs:build:local": "pnpm -F docs run build:local",
    "docs:build:github": "pnpm -F docs run build:github",
    "docs:build:npm": "pnpm -F docs run build:npm",
    "docs:preview": "pnpm -F docs run docs:preview",
    "prepublishOnly": "echo '\\x1b[34mprepublishOnly\\x1b[39m'; pnpm run refresh && pnpm run docs:build:npm"
  },
  "dependencies": {
    "@inquirer/prompts": "^5.0.1",
    "commander": "^12.0.0",
    "ejs": "^3.1.10",
    "serialize-javascript": "^6.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.2",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/chrome": "^0.0.266",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "cross-env": "^7.0.3",
    "eslint": "9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.25.0",
    "husky": "^9.0.11",
    "markdownlint-cli2": "^0.13.0",
    "postcss-html": "^1.6.0",
    "prettier": "^3.2.5",
    "rollup": "^4.16.0",
    "rollup-plugin-dts": "^6.1.0",
    "stylelint": "^16.3.1",
    "typescript": "5.4.5",
    "typescript-eslint": "7.7.0",
    "vue-eslint-parser": "^9.4.2"
  },
  "exports": {
    ".": {
      "browser": "./dist/browser/index.js",
      "node": "./dist/node/index.js"
    },
    "./base": {
      "browser": "./dist/browser/base.js",
      "node": "./dist/node/base.js"
    },
    "./dev": {
      "browser": "./dist/browser/dev.js",
      "node": "./dist/node/dev.js"
    },
    "./storage": {
      "browser": "./dist/browser/storage.js",
      "node": "./dist/node/storage.js"
    },
    "./*": "./*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Feawaris/hp-shared.git"
  },
  "homepage": "https://github.com/Feawaris/hp-shared",
  "unpkg": "./dist/browser/index.umd.js",
  "jsdelivr": "./dist/browser/index.umd.js"
}
