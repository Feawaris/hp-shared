{
  "name": "hp-shared",
  "version": "1.5.0",
  "license": "MIT",
  "bin": "./bin/index.cjs",
  "scripts": {
    "refresh": "pnpm install && husky && pnpm run build",
    "update": "npx ncu --target newest --upgrade && pnpm run refresh",
    "clean:ignoredConfigFiles": "npx rimraf .markdownlint.json .markdownlint-cli2.cjs .markdownlint-cli2.cjs .markdownlintignore stylelint.config.cjs .stylelintrc.json .stylelintignore eslint.config.cjs .eslintrc.cjs .eslintignore prettier.config.cjs .prettierignore commitlint.config.cjs",
    "lint": "pnpm run lint:ignore ; pnpm run lint:md ; pnpm run lint:css ; pnpm run lint:js ; pnpm run lint:prettier ; pnpm run lint:git",
    "lint:fix": "pnpm run lint:ignore ; pnpm run lint:md:fix ; pnpm run lint:css:fix ; pnpm run lint:js:fix ; pnpm run lint:prettier:fix ; pnpm run lint:git",
    "lint:ignore": "node config/lint-ignore.cjs",
    "lint:md": "node config/lint-md.cjs && markdownlint-cli2 '**/*.{md,markdown}'",
    "lint:md:fix": "node config/lint-md.cjs && markdownlint-cli2 '**/*.{md,markdown}' --fix",
    "lint:css": "node config/lint-css.cjs && stylelint '**/*.{css,vue}'",
    "lint:css:fix": "node config/lint-css.cjs && stylelint '**/*.{css,vue}' --fix",
    "lint:js": "node config/lint-js.cjs && eslint",
    "lint:js:fix": "node config/lint-js.cjs && eslint --fix",
    "lint:prettier": "node config/lint-prettier.cjs && prettier --check .",
    "lint:prettier:fix": "node config/lint-prettier.cjs && prettier --check . --write",
    "lint:git": "node config/lint-git.cjs && echo 'test: commitlint' | commitlint",
    "build": "rollup --config --bundleConfigAsCjs",
    "build:watch": "pnpm run build --watch",
    "docs:dev": "cross-env vuepress dev docs --clean-cache",
    "docs:build:local": "cross-env vuepress_to=local vuepress build docs",
    "docs:build:github": "cross-env vuepress_to=github vuepress build docs",
    "docs:build:npm": "cross-env vuepress_to=npm vuepress build docs",
    "docs:preview": "npx serve dist/docs",
    "prepublishOnly": "echo '\\x1b[34mprepublishOnly\\x1b[39m'; pnpm run refresh && pnpm run docs:build:npm"
  },
  "dependencies": {
    "@inquirer/prompts": "^4.3.2",
    "commander": "^12.0.0",
    "ejs": "^3.1.9",
    "serialize-javascript": "^6.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@typescript-eslint/eslint-plugin": "^7.5.1-alpha.10",
    "@typescript-eslint/parser": "^7.5.1-alpha.10",
    "@vue/repl": "^4.1.1",
    "@vuepress/bundler-vite": "2.0.0-rc.9",
    "@vuepress/plugin-docsearch": "2.0.0-rc.24",
    "chart.js": "^4.4.2",
    "cross-env": "^7.0.3",
    "echarts": "^5.5.0",
    "eslint": "9.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.24.0",
    "flowchart.ts": "^3.0.0",
    "husky": "^9.0.11",
    "katex": "^0.16.10",
    "kotlin-playground": "^1.30.0",
    "markdownlint-cli2": "^0.13.0",
    "markmap-lib": "^0.16.1",
    "markmap-toolbar": "^0.16.0",
    "markmap-view": "^0.16.0",
    "mermaid": "^11.0.0-alpha.7",
    "postcss-html": "^1.6.0",
    "prettier": "^3.2.5",
    "reveal.js": "^5.0.5",
    "rollup": "^4.14.1",
    "sandpack-vue3": "^3.1.11",
    "stylelint": "^16.3.1",
    "typescript": "5.5.0-dev.20240405",
    "typescript-eslint": "7.5.1-alpha.10",
    "vidstack": "^1.11.11",
    "vue": "^3.4.21",
    "vue-eslint-parser": "^9.4.2",
    "vuepress": "2.0.0-rc.9",
    "vuepress-theme-hope": "2.0.0-rc.34"
  },
  "exports": {
    ".": {
      "browser": "./src/index-browser.js",
      "node": "./dist/node/index.js"
    },
    "./base": {
      "browser": "./src/base/index.js",
      "node": "./dist/node/base.js"
    },
    "./dev": {
      "browser": "./src/dev/index.js",
      "node": "./dist/node/dev.js"
    },
    "./storage": {
      "browser": "./src/storage/index-browser.js",
      "node": "./dist/node/storage.js"
    },
    "./*": "./*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Feawaris/hp-shared.git"
  },
  "homepage": "https://github.com/Feawaris/hp-shared",
  "unpkg": "./dist/browser/index.umd.js",
  "jsdelivr": "./dist/browser/index.umd.js"
}
