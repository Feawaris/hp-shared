/**
 * [tsconfig](https://www.typescriptlang.org/tsconfig/)
 */
{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": false,
    "strict": false,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "noEmitOnError": false,
    "skipLibCheck": true,
    "allowImportingTsExtensions": true,
    "resolveJsonModule": true
  }
}
